                "settings": {
                    "execution_mode": {
                        "value": "Validate & Execute"
                    },
                    "soqlQuery": {
                        "expression": false,
                        "value": "SELECT Id, Owner.Name, Name, Account.Name, Amount, CloseDate, StageName, LeadSource, Type, Region_from_Account__c FROM Opportunity WHERE CloseDate = LAST_FISCAL_QUARTER AND Amount > 0 AND Type IN ('Add-on Business', 'New Business', 'Renewal') AND StageName IN ('6 - Closed (Won)', '4 - Closed Won') ORDER BY CloseDate DESC"
                    }
                },
                "view_serial": 100
            }
