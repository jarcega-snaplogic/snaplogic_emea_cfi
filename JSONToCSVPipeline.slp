                                        "expression": {
                                            "expression": true,
                                            "value": "$customer_info.name != null ? $customer_info.name : _defaultCustomerName"
                                        },
                                        "targetPath": {
                                            "value": "customer_name"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$customer_info.email"
                                        },
                                        "targetPath": {
                                            "value": "email_address"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$customer_info.phone"
                                        },
                                        "targetPath": {
                                            "value": "phone_number"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$order_details.product_name"
                                        },
                                        "targetPath": {
                                            "value": "product"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$order_details.quantity"
                                        },
                                        "targetPath": {
                                            "value": "qty"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$order_details.unit_price"
                                        },
                                        "targetPath": {
                                            "value": "price_per_unit"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$order_details.total_amount >= _minimumOrderAmount ? $order_details.total_amount : 0.00"
                                        },
                                        "targetPath": {
                                            "value": "order_total"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$order_date != null ? Date.parse($order_date).toLocaleDateString() : \"\""
                                        },
                                        "targetPath": {
                                            "value": "order_date_formatted"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$shipping_address.street + \", \" + $shipping_address.city + \", \" + $shipping_address.state + \" \" + $shipping_address.zip_code"
                                        },
                                        "targetPath": {
                                            "value": "shipping_full_address"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$shipping_address.country"
                                        },
                                        "targetPath": {
                                            "value": "country"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$status != null && $status.length() > 0 ? $status.toUpperCase() : \"UNKNOWN\""
                                        },
                                        "targetPath": {
                                            "value": "order_status"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$tracking_number != null ? $tracking_number : \"N/A\""
                                        },
                                        "targetPath": {
                                            "value": "tracking_id"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "$order_details.total_amount > 100 ? \"HIGH_VALUE\" : \"STANDARD\""
                                        },
                                        "targetPath": {
                                            "value": "order_priority"
                                        }
                                    },
                                    {
                                        "expression": {
                                            "expression": true,
                                            "value": "Date.now().toLocaleDateString()"
                                        },
                                        "targetPath": {
                                            "value": "processed_date"
                                        }
                                    }
                                ]
                            }
                        }
