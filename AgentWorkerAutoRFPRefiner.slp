bmissions\",\n                \"parameters\": [\n                    {\n                        \"name\": \"Requirement\",\n                        \"type\": \"STRING\",\n                        \"description\": \"An RFX Question about snapLogic\",\n                        \"required\": true\n                    }\n                ]\n            },\n            {\n                \"sl_type\": \"function\",\n                \"name\": \"RetrieverAnalystRFIsApi\",\n                \"description\": \"Searches a library of curated RFP answers from previous analyst submissions\",\n                \"parameters\": [\n                    {\n                        \"name\": \"Requirement\",\n                        \"type\": \"STRING\",\n                        \"description\": \"An RFX Question about snapLogic\",\n                        \"required\": true\n                    }\n                ]\n            },\n            {\n                \"sl_type\": \"function\",\n                \"name\": \"LoopioUpdateEntryApi\",\n                \"description\": \"Update a Loopio entry identified by an id, which will include a question and a text response and may include one or several compliance answers.\",\n                \"parameters\": [\n                    {\n                        \"name\": \"id\",\n                        \"type\": \"INTEGER\",\n                        \"description\": \"the loopio entry id\",\n                        \"required\": true\n                    },\n                    {\n                        \"name\": \"question\",\n                        \"type\": \"STRING\",\n                        \"description\": \"the requirement that needs answering\",\n                        \"required\": true\n                    },\n                    {\n                        \"name\": \"text\",\n                        \"type\": \"STRING\",\n                        \"description\": \"the answer to the requirement as text, it can be enriched with code for 