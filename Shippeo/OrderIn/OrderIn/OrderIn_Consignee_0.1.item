    @JsonProperty("dates")
    public List<OrderIn_ShippeoDate> getDates() {
        return dates;
    }

    @JsonProperty("dates")
    public void setDates(List<OrderIn_ShippeoDate> dates) {
        this.dates = dates;
    }
    
    public void addDate(OrderIn_ShippeoDate date) {
    	if(Relational.ISNULL(date)) return;
    	if(this.getDates()==null) this.setDates(new ArrayList<OrderIn_ShippeoDate>());
    	if(!date.getQualifier().equals("64") && !date.getQualifier().equals("63")) {
    		List<OrderIn_ShippeoDate> newDateList = new ArrayList<OrderIn_ShippeoDate>();
    		for(OrderIn_ShippeoDate d:this.getDates()) {
    			if(!((d.getQualifier().equals("64") || d.getQualifier().equals("63")) && d.getDateTime() == null)) {
    				newDateList.add(d);
    			}
    		}
    		this.getDates().clear();
    		this.setDates(newDateList);
    	}
    	else if(date.getDateTime()== null) {
    		for(OrderIn_ShippeoDate d:this.getDates()) {
    			if(!d.getQualifier().equals("64") && !d.getQualifier().equals("63")) {
    				return;
    			}
    		}
    	}
    	this.getDates().add(date);
    }
    
    @JsonAnyGetter
    public Map<String, Object> getAdditionalProperties() {
        return this.additionalProperties;
    }

    @JsonAnySetter
    public void setAdditionalProperty(String name, Object value) {
        this.additionalProperties.put(name, value);
    }

}
